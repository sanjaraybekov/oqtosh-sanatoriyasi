+import React from 'react';
+import {motion} from "framer-motion/dist/framer-motion";
+
+const PageAnimation = ({children, inEffect = true, outEffect = true}) => {
+    const pageTransitionVariants = {
+        initial: {
+            y: inEffect ? '0' : '-100%',
+        },
+        animate: {
+            // y: inEffect ? '-50%' : '-50%',
+            y: inEffect ? '-100%' : '-100%',
+        },
+        exit: {
+            // y: '-100%',
+            y: outEffect ? '0' : '-100%',
+            // transition: {duration: outEffect ? 0.5 : 0}
+        }
+    };
+    return (
+        <>
+            <div className='page-transition'>
+                {[...Array(5).keys()].map((id) => (
+                    <motion.div
+                        className='page-transition__block'
+                        initial='initial'
+                        animate='animate'
+                        // exit={{
+                        //     y:-500
+                        // }}
+                        exit='exit'
+                        transition={{
+                            duration: 0.5,
+                            delay: id * 0.1,
+                            ease: 'easeInOut',
+                        }}
+                        key={id}
+                        variants={pageTransitionVariants}
+                        style={{left: `${id * 20}vw`}}/>
+                ))}
+            </div>
+            {children}
+        </>
+    );
+};
+
+export default PageAnimation;
